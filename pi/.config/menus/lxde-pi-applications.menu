        <MergeFile type="parent">/etc/xdg/menus/lxde-pi-applications.menu</MergeFile>
        <Menu>
                <Name>Development</Name>
                <DirectoryDir>/home/pi/.local/share/desktop-directories</DirectoryDir>
        </Menu>
        <Menu>
                <Name>Education</Name>
                <DirectoryDir>/home/pi/.local/share/desktop-directories</DirectoryDir>
        </Menu>
        <Menu>
                <Name>Office</Name>
                <DirectoryDir>/home/pi/.local/share/desktop-directories</DirectoryDir>
        </Menu>
        <Menu>
                <Name>Internet</Name>
                <DirectoryDir>/home/pi/.local/share/desktop-directories</DirectoryDir>
        </Menu>
        <Menu>
                <Name>Multimedia</Name>
                <DirectoryDir>/home/pi/.local/share/desktop-directories</DirectoryDir>
        </Menu>
        <Menu>
                <Name>Graphics</Name>
                <DirectoryDir>/home/pi/.local/share/desktop-directories</DirectoryDir>
        </Menu>
        <Menu>
                <Name>Games</Name>
                <DirectoryDir>/home/pi/.local/share/desktop-directories</DirectoryDir>
        </Menu>
        <Menu>
                <Name>Other</Name>
                <DirectoryDir>/home/pi/.local/share/desktop-directories</DirectoryDir>
        </Menu>
        <Menu>
                <Name>DesktopSettings</Name>
                <DirectoryDir>/home/pi/.local/share/desktop-directories</DirectoryDir>
        </Menu>
